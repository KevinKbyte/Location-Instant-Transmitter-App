<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.nelso_8bi0ds3.testlocationapp4.MainActivity">

    <Button
        android:id="@+id/alert"
        android:layout_width="137dp"
        android:layout_height="74dp"
        android:backgroundTint="@android:color/holo_red_dark"
        android:shadowColor="@color/common_google_signin_btn_text_dark_focused"
        android:text="@string/send_alert"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        tools:layout_editor_absoluteX="124dp"
        tools:layout_editor_absoluteY="218dp"
        tools:textColorHighlight="@color/common_google_signin_btn_text_dark_focused" />

    <TextView
        android:id="@+id/latitudeID"
        android:layout_width="155dp"
        android:layout_height="45dp"
        android:text="@string/extracted_latitude"
        tools:layout_editor_absoluteX="211dp"
        tools:layout_editor_absoluteY="34dp" />

    <TextView
        android:id="@+id/longitudeID"
        android:layout_width="155dp"
        android:layout_height="45dp"
        android:text="@string/extracted_longitude"
        tools:layout_editor_absoluteX="213dp"
        tools:layout_editor_absoluteY="110dp" />

    <TextView
        android:id="@+id/latitudeText"
        android:layout_width="155dp"
        android:layout_height="45dp"
        android:text="@string/latitude"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="34dp"
        tools:textColor="@color/colorPrimaryDark"
        tools:textSize="24sp" />

    <TextView
        android:id="@+id/longitudeText"
        android:layout_width="155dp"
        android:layout_height="45dp"
        android:text="@string/longitude"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="110dp"
        tools:textColor="@color/colorPrimaryDark"
        tools:textSize="24sp" />

</android.support.constraint.ConstraintLayout>
